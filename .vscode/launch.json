{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Flutter Web (Arc)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["-d", "web"],
          },
        {
            "name": "bnflp_wireframe",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "bnflp_wireframe (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "bnflp_wireframe (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}